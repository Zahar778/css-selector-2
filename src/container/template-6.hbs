<header class="page_block">
  <div class="header row g-0">
    <div class="col-3">
      {{> logo}}
    </div>
    <nav class="header_nav col-9">
      <div class="header_nav_right">
        {{>nav list=header}}
      </div>
    </nav>
  </div>
</header>





  <footer class="row footer">
    <div>
      {{> logo}}
      <span class="footer_copyright"> Copyright 2023-2024 </span> 
    </div>
    {{#each footer}}
    <div class="col-3">
      <span class="footer_title">{{title}}</span>
      {{>nav list=links isVertical=true}}
    </div>
    {{/each}}
  </footer>
</footer>
